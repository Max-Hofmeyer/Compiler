//keywords
int main() {
    char c = 'a';
    if (c == 'a') {
        return 0;
    } else {
        return 1;
    }
    for (int i = 0; i < 10; i++) {
        write("Loop iteration");
    }
    do {
        read(c);
    } while (c != 'q');
    switch (c) {
        case 'a':
            write("A");
            break;
        default:
            write("Other");
    }
    continue;
    break;
    newline;
}

